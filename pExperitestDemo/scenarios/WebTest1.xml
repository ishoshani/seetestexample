<command appiumName="InstallApp(path)" enable="true" name="Install(appPath,instrument,keepData)">
<parameters>
<parameter appium="true" defaultValue="com.experitest.Browser" description="can be an APK/IPA etc. absolute path or an activity name from the application manager." name="Path" pattern="(.+)" type="install"/>
