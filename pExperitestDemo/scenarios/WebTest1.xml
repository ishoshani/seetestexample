</parameters>
</command>
<command appiumName="Launch(app)" enable="true" name="Launch(activity, instrument, stopIfRunning)">
<parameters>
<parameter appium="true" defaultValue="experibrowser:http://www.cnn.com" description="The application main activity or URL" name="Activity/URL" pattern="(.+)" type="launch"/>
<parameter appium="false" defaultValue="true" description="If set to true then will launch in instrument mode" name="Instrument" pattern="(true|false)" type="bool"/>
<parameter appium="false" defaultValue="false" description="If set to true then will stop the running process before launching it" name="Stop If Running" pattern="(true|false)" type="bool"/>
</parameters>
</command>
<command appiumName="FindElement and Click(Element)" enable="true" name="Click(Zone, Element, Index, ClickCount)">
<parameters>
